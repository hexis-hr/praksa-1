<!doctype html>

<html lang='en' itemscope itemtype="http://schema.org/LocalBusiness">

<head>
    <meta charset='utf-8'>
    <!-- Fonts START -->
    <link rel='dns-prefetch' href='https://fonts.googleapis.com'>
    <!-- Fonts STOP -->

    <meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'>
    <meta name='viewport' content='width=device-width, height=device-height, initial-scale=1.0, maximum-scale=1.0'>
    <meta name='msapplication-tap-highlight' content='no'>

    <title>Homepage</title>
    {% block stylesheets %}
        {# 'app' must match the first argument to addEntry() in webpack.config.js #}
        {{ encore_entry_link_tags('app') }}
        <link rel="stylesheet" href="../assets/styles/main.min.css" type='text/css'>
        <link rel='dns-prefetch' href='https://fonts.googleapis.com'>
        <link rel="shortcut icon" href="./assets/media/favicon.png"/>

        <!-- Renders a link tag (if your module requires any CSS)
             <link rel="stylesheet" href="/build/app.css"> -->
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
        <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js"></script>
        <!-- Renders app.js & a webpack runtime.js file
            <script src="/build/runtime.js" defer></script>
            <script src="/build/app.js" defer></script>
            See note below about the "defer" attribute -->
    {% endblock %}
    <meta name="description" content="Login page">

    <!-- Schema.org markup for Google+ -->
    <meta itemprop="name" content="Homepage">
    <meta itemprop="description" content="Page description. No longer than 155 characters.">
    <meta itemprop="image" content="../media/brand/ogp.png">


    <!-- Twitter Card data -->
    <meta name="twitter:card" content="product">
    <meta name="twitter:site" content="@site_handle">
    <meta name="twitter:title" content="Homepage">
    <meta name="twitter:description" content="Page description. No longer than 155 characters.">
    <meta name="twitter:creator" content="@author_handle">
    <meta name="twitter:image" content="../media/brand/ogp.png">


    <!-- Open Graph data -->
    <meta property="og:title" content="Homepage">
    <meta property="og:type" content="">
    <meta property="og:url" content="http://www.example.com/">
    <meta property="og:image" content="../media/brand/ogp.png">
    <meta property="og:description" content="Page description. No longer than 155 characters.">
    <meta property="og:site_name" content="">

    <link rel="shortcut icon" href="../media/brand/favicon.ico"/>
    <link rel="shortcut icon" href="../media/favicon.png"/><!--Favicon-->

    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="theme-color" content="#ffffff">

    <!-- Synchronously font loading -->
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js"></script>
    <script>
        WebFont.load({
            google: {
                families: ['Nunito+Sans:400,700,900']
            }
        });
    </script>





    <!-- Critical CSS -->
    <link rel="stylesheet" href="../css/main.min.css" type='text/css'>

    <!-- Secondary Meta Tags -->
    <meta name='description' content=''>
</head>

<body class="p-login">


<main class="bg-secondary py-5 d-flex align-items-center">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-4">
                <div class="d-flex flex-column align-items-center">
                    <!-- <div class="c-img -logo mb-5" style="background-image: url('media/brand/logo-full.svg')"></div> Place for app logo-->

                    <div class="f-20 mb-5 color-white"><!--Application name-->Extract</div>

                    <div class="row justify-content-center">
                        <div class="col-lg-10">




                            <form class="row"  action="dashboard.html"><!--Next page to load after logging in (Currently not existing)-->
                                <div class="col-12">


                                    <div class="form-group mb-3">


                                        <!--Email-->
                                        <label class="sr-only" for="loginEmail">Email</label>
                                        <input id="loginEmail"
                                               class="form-control"
                                               type="email"
                                               name="loginForm"
                                               placeholder="Email"
                                        >


                                    </div>
                                </div>

                                <div class="col-12">


                                    <div class="form-group mb-3">

                                        <!--Password-->
                                        <label class="sr-only" for="loginPass">Password</label>
                                        <input id="loginPass"
                                               class="form-control"
                                               type="text"
                                               name="loginForm"
                                               placeholder="Password"
                                        >


                                    </div>
                                </div>

                                <div class="col-12">
                                    <button class="btn btn-primary w-100 mb-3" type="submit">Log in</button>
                                </div>

                                <div class="col-12">

                                    <!-- Note for DEVs: error message below-->
                                    <div class="btn btn-danger w-100 mt-4">Invalid crendetials</div>
                                </div>

                            </form>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</main>

